#include <iostream>
using namespace std;

long long int mex=10000009;
bool arr[10000009];

int main()
{
    long long int l,r,i;
   
    cin>>l>>r;
   
    long long int cnt=0;
    for(i=0;i<mex;i++)
    arr[i]=true;
   
    arr[0]=false;
   
    arr[1]=false;
   
    for(int i=2;i*i<=mex;i++)
    {
    	if(arr[i]==true)
    	{
        for(int j=2*i;j<=mex;j+=i)
        {
        arr[j]=false;
        }
        }
    }
   
    for(int i=l;i<r;i++)
    {
        if(arr[i]==true) cnt++;
    }
   
    cout<<cnt<<endl;
}
